  "jest": {
    "collectCoverageFrom": [
      "<rootDir>/src/client/components/**/*.{js,jsx}"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/client/setup-tests.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
